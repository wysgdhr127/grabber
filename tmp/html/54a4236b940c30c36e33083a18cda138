<script>
Yobj.ready(function(){
	if(/iPhone|iPad|Android|Mobile/i.test(navigator.userAgent)){return $$('#main_guide_icon').hide();}
	var guide = new Yobj.lib.Guide({
		mask:"div.guide_mask,div.guide_hd,div.guide_bg,div.main_guide",
	   items:"div.step",
	   close:"a.close,a.goto-last",
	    next:"a.goto-next",
	    type:'jczq2',
	    auto:false,
	    login:true,
		effect:false,
	  	onshow: function(n,lay){
		    $$('body').addClass('ie6_mask_hide_select');
		    lay = lay.find('div.step_in');
			if (lay.attr('data-bgstyle') && lay.size()) {
			    lay.one(0).style.cssText = lay.attr('data-bgstyle');
			    lay.attr('data-bgstyle', '');
		    }
		},
		onclose: function(){
			$$('body').removeClass('ie6_mask_hide_select');
		}
	});
	$$('#main_guide_icon a').click(function(){
        guide.goto(0);
        return false;
	});
	guide.onchange = function(n){
		if(n==4){
		    $$('#guide_bg').setStyle('backgroundImage', 'url(http://www.500cache.com/trade/images/zc/jczq_js_bg.png)');
		}else{
		    $$('#guide_bg').setStyle('backgroundImage', 'url(http://www.500cache.com/trade/images/zc/jczq_bg.png)');
		}
	};
	$$('#main_guide div.guide_page').each(function(div, i){
        $$(div).find('a').each(function(el, j){
        	el.guide_index = j;
        }).click(function(){
			guide.goto(this.guide_index);
	        return false;        
        });
	});
})
</script>
</body>
<script>
namespace("Eop", function (){
	var com = this.com = JS.StdClass({
		login: function (callback){JS.Y.postMsg('msg_login', JS.func(callback));},
		logout: function (callback){JS.Y.postMsg('msg_logout', JS.func(callback));},
		alert: function (){JS.Y.alert.apply(JS.Y, arguments);	},
		confirm: function (){JS.Y.confirm.apply(JS.Y, arguments);},
		user: {
			getName: function (){return Class.C('userName');},
			getMoney: function (){return Class.C('userMoney') || 0;},
			getCreateTime: function (){return Class.C('userCreateTime');},
			getHBEndTime: function (){return Class.C('hbEndTime');},
			getHBMoney: function (){return Class.C('userHBMoney') || 0;}
		}
	});
	JS.Y.onMsg('msg-login-ok', function (fn){com.trigger('login', fn);});
	JS.Y.onMsg('user-info-loaded', function (fn){com.trigger('userInfo', fn);});
});
namespace("Eop.lots", {
	checkHm: function (){
		
	}
});

/* Clock 当前时间
------------------------------------------------------------------------------*/
Class( 'Clock', {
    getStdDate: function  (s){
        var d = 0;
        if (s) {
            d = new Date() - new Date(typeof s == 'string' ? (s.replace(/-/g, '/')) : s);
            this.diff = d;
        }
        return function (t){
            var now = new Date(new Date() - d); 
            return now;
        };
    },
	index : function(clock_id) {
		this.clockTag = this.get(clock_id);
		var self = this;
        self.updateTime(self);
	},
    updateTime: function (self){
        Y.ajax({
            url: '/inc/servertime.php?_='+Math.random(),
            end: function (data){
                var getTime = self.getStdDate((data.text+'').trim());
                self.runClock(getTime);
            }
        });
    },
	runClock : function(getTime) {
		var self = this, diff = this.diff, startDate = new Date().getTime();
        clearInterval(this.timerId);
        function zero(n){
            return parseInt(n) < 10 ? '0' + n : n;
        }
		this.timerId = setInterval( function() {
            startDate+=1000;
            var d=getTime(), locale_diff = Math.abs(new Date() - new Date(startDate));
            if (locale_diff > 1800) {
                self.updateTime(self);
            }
			var d_str = zero((d.getMonth() + 1)) + '月' + zero(d.getDate()) + '日 ' +zero(d.getHours()) + ':' + zero(d.getMinutes()) + ':' + zero(d.getSeconds());
			self.clockTag.html(d_str);
		}, 1000);
	}
} );
Y.lib.Clock('#running_clock')
</script>
</html>
