<script language="javascript">
function guide(flag)
{
	var guide=document.getElementById("guide");
	guide.innerHTML="&nbsp;&gt; "+flag;
}
function do_statistics(button_c, button_n) {
	var button_t, button_w, button_b;
	button_c = parseInt(button_c,10);
	button_t = button_c - button_c % 1000000;
	button_w = button_c - button_c % 10000;
	button_b = button_c - button_c % 100;
	if (typeof(dcsMultiTrack) == 'function') {
		dcsMultiTrack('DCSext.button_t', button_t, 'DCSext.button_w', button_w, 
		              'DCSext.button_b', button_b, 'DCSext.button_c', button_c, 
		              'DCSext.button_n', button_n );
	}
}
</script>
