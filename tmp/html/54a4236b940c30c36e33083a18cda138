<script type="text/javascript">
Class("sendtomsg",{
	ready:true,
	index:function(){
		Yobj.C('pos',1);
        if(Yobj.C('send-web-msg')){
            this.tosendmsg(1)
        }else if(Yobj.C('send-web-msg1')){
        	Yobj.C('pos',2);
        	this.tosendmsg(2)
        }else{
            this.onMsg('send-web-msg', function(){
                this.tosendmsg(1)
            }); 
            this.onMsg('send-web-msg1', function(){
            	Yobj.C('pos',2);
                this.tosendmsg(2)
            });           
        }
	},
	tosendmsg:function(pos){
	    var sendurl,usermobile,Y=this;
	    if(pos == 1)
			usermobile = Y.one('#tosend_mobile').value;
	    else
	    	usermobile = Y.one('#tosend_mobile1').value;
		if(usermobile.length<11 || usermobile.length>12){
			this.show_message("手机号码错误");
			return;
		}
		window.showmsg = function (data){
			Y.show_message(data.msg)
		}
		sendtourl = "/main/ajax/sendmsg_ajax.php?action=sendmsg&mobile="+usermobile+"&rnd="+(+new Date());
		this.loadScript(sendtourl);
	},
    show_message:function(data){
	    if(Yobj.C('pos') == 1)
			Y.one("#send_msg").innerHTML = data;
	    else
	    	Y.one("#send_msg1").innerHTML = data;
	}
});
Y.ready(function(){
	Y.get('#showhbmoney').on('click', function() {
		var t = Y.get('#showhbmoney a'), th = t.html();
		if(th == '显示') {
			t.html('隐藏');
			Y.get('#xhbmoney, #showmoney').hide();
			Y.get('#hidehbmoney, #hidemoney').show();
		} else {
			t.html('显示');
			Y.get('#hidehbmoney, #hidemoney').hide();
			Y.get('#xhbmoney, #showmoney').show();
		}
	});
	Y.use('http://market.500.com/huodong/mask/mask.php');
	Y.use('http://space.500.com/port/request.php?c_limit=1&c_id=0024&rnd='+(+new Date));
});
Class("huodongdiv",{
	ready:true,
	index:function(){
		var Y = this,
			wcn = Y.get('#right_float_nav'),
			temph = 300;
		wcn.show();
		resize_window(wcn);
		Y.get(window).resize(function(){
           resize_window(wcn);
        });
        function resize_window(wcn){
        	 var w = Math.max(document.body.clientWidth, document.documentElement.clientWidth);
            if(w<1160){
                wcn.hide();
            }else{
                wcn.show();
            }
            var _l = w/2 + 524;
            if(Y.get('#main_guide_icon').size() > 0){
            	var main_guide_icon_left = Y.get('#main_guide_icon').getXY().x;
            	_l = _l < main_guide_icon_left?main_guide_icon_left:_l;
            }
            wcn.setStyle('left',_l+'px');
        }
		var timer = setInterval(function(){
			if(Y.get('div.jSemFloat_R').size() > 0 && Y.get('#right_float_nav').getStyle('display') == 'block'){
				Y.get('div.jSemFloat_R').setStyle('top','540px');
				Y.get('div.jSemFloat_R').setStyle('visibility','visible');
				clearInterval(timer);
			}
		},500);
		Y.get('#wcn_close').on('click',function(){
			Y.get('ul.wcn_nav_list').hide();
			Y.get('#right_float_nav').setStyle('height','89px');
			Y.get(this).hide();
			if(Y.get('div.jSemFloat_R').size() > 0){
				var jSemFloat_Rh = Y.get('div.jSemFloat_R').getXY().y;
				Y.get('div.jSemFloat_R').setStyle('top',(jSemFloat_Rh-temph)+'px');
				Y.get('div.jSemFloat_R').setStyle('visibility','visible');					
			}
		});
		Y.get('#right_float_nav div.wcn_nav_show2').on('click',function(){
			if(Y.get('#wcn_close').getStyle('display') == 'none'){
				Y.get('ul.wcn_nav_list').show();
				Y.get('#right_float_nav').setStyle('height','460px');
				Y.get('#wcn_close').show();
				if(Y.get('div.jSemFloat_R').size() > 0){
					var jSemFloat_Rh = Y.get('div.jSemFloat_R').getXY().y;
					Y.get('div.jSemFloat_R').setStyle('top',(jSemFloat_Rh+temph)+'px');				
				}
			}
		});
	}
});
</script>
